{
  "expo": {
    "name": "My PCSO Lotto Random Picker",
    "slug": "MyLottoRandomPicker",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/pcso-batman-logo.png",
    "splash": {
      "image": "./assets/splash-batman.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package" : "com.batman.mylottorandompicker",
      "versionCode" : 2,
      "adaptiveIcon": {
        "foregroundImage": "./assets/pcso-batman-logo.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
